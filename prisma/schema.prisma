generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model produits {
  id    Int     @id
  name  String? @db.VarChar(100)
  stock Int?
  price Float?  @db.Float
}

model users {
  id       Int     @id
  name     String? @db.VarChar(100)
  email    String? @db.VarChar(100)
  password String? @db.VarChar(100)
}
